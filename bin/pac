#!/usr/bin/env rc

fn usage {
	printf 'usage: pac install\n'
	printf '       pac fetch <name> <shasum> <url>\n'
	printf '       pac env\n'
	exit 1
}

# Usage: pac <subcommand>
if (~ $#* 0) {
	usage
}

# Build subcommand
sc = pac-$1

# Discard first argument
shift

if (which -s $sc) {
	$sc $*
} else {
	usage
}
